{
  "name": "rntv",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "web": "IS_WEB=true && expo start --web",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build": "cd android && ./gradlew assembleRelease",
    "fix:assets": "find desktop/web-build -type f -name \"*.js\" -exec sed -i 's#\"/assets#\"assets#g' {} +",
    "fix:index": "sed -i 's#/_expo#_expo#g' desktop/web-build/index.html",
    "build:web": "IS_WEB=true && expo export --platform web --output-dir desktop/web-build && yarn fix:index && yarn fix:assets",
    "build:desktop": "yarn build:web && cd desktop && yarn package"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^6.6.0",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.10.0",
    "aes-js": "^3.1.2",
    "axios": "^1.6.7",
    "buffer": "^6.0.3",
    "cheerio": "^1.0.0-rc.12",
    "crypto-js": "^4.2.0",
    "expo": "^51.0.21",
    "i18n-js": "^4.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "npm:react-native-tvos@0.74.3",
    "react-native-blob-util": "^0.19.6",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-tvos": "0.74.1-0",
    "react-native-video": "6.1.2",
    "react-native-web": "~0.19.10",
    "video.js": "^8.16.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.8",
    "@babel/preset-env": "^7.24.8",
    "@babel/runtime": "^7.24.8",
    "@react-native/babel-preset": "^0.74.85",
    "@react-native/eslint-config": "0.74.85",
    "@react-native/metro-config": "0.74.85",
    "@react-native/typescript-config": "0.74.85",
    "@types/aes-js": "^3.1.1",
    "@types/crypto-js": "^4.2.1",
    "@types/jest": "^29.2.1",
    "@types/react": "18.2.79",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-preset-expo": "^11.0.12",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.3.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.2.2"
}